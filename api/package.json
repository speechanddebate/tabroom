{
  "name": "indexcards",
  "version": "1.0.0",
  "description": "Tabroom.com Node API",
  "main": "wrap.js",
  "scripts": {
    "start": "NODE_ENV='production' NODE_OPTIONS='--max_old_space_size=8000' node --use_strict wrap.js",
    "production": "NODE_ENV='production' NODE_OPTIONS='--max_old_space_size=8000' pm2 -i 4 start wrap.js --name indexcards --node-args --use_strict -o /var/log/indexcards/output.log -e /var/log/indexcards/error.log ",
    "staging": "NODE_ENV='staging' NODE_OPTIONS='--max_old_space_size=1000' pm2 -i 4 start wrap.js --name indexcards --node-args --use_strict -o /var/log/indexcards/output.log -e /var/log/indexcards/error.log ",
    "stagetest": "NODE_ENV='stagetest' nodemon --require esm --use-strict wrap.js",
    "dev": "NODE_ENV='development' nodemon --require esm --use-strict wrap.js",
    "test": "NODE_ENV='test' mocha 'indexcards/**/*.test.js'",
    "test-live": "NODE_ENV='test' TEST_LIVE=true NOCK_OFF=true mocha 'indexcards/**/*.test.js'",
    "test-code-build": "node -e '2 === 2'",
    "test-quick": "NODE_ENV='test' mocha --grep @slow --invert 'indexcards/**/*.test.js'",
    "cover": "rm -fr ./node_modules/.cache; NODE_ENV='test' nyc mocha 'indexcards/**/*.test.js'",
    "cover-quick": "rm -fr ./node_modules/.cache; NODE_ENV='test' nyc mocha --grep @slow --invert 'indexcards/**/*.test.js'",
    "lint": "eslint --ignore-path ../.gitignore ."
  },
  "repository": {
    "type": "git",
    "url": "git+https : //github.com/speechanddebate/tabroom.git"
  },
  "keywords": [
    "speech",
    "debate",
    "congress",
    "tabulation",
    "forensics",
    "NSDA"
  ],
  "author": "Chris Palmer",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/speechanddebate/tabroom/issues"
  },
  "engines": {
    "node": ">= 12.16.1 <= 14.8.0"
  },
  "homepage": "https://www.tabroom.com/about",
  "dependencies": {
    "@alt3/sequelize-to-json-schemas": "^0.3.55",
    "argon2": "^0.26.1",
    "basic-auth": "^2.0.1",
    "body-parser": "^1.18.3",
    "bottleneck": "^2.19.0",
    "cookie-parser": "^1.4.4",
    "crypt3": "^1.0.0",
    "crypto-js": "^4.0.0",
    "csv-stringify": "^5.3.0",
    "dotenv": "^8.2.0",
    "esm": "^3.2.22",
    "express": "^4.17.1",
    "express-openapi": "^4.6.1",
    "express-rate-limit": "^3.4.1",
    "express-winston": "^3.1.0",
    "full-name-splitter": "^1.1.1",
    "helmet": "^3.16.0",
    "isomorphic-fetch": "^3.0.0",
    "jsonwebtoken": "^8.5.1",
    "mariadb": "^2.5.2",
    "moment-timezone": "^0.5.33",
    "nodemailer": "^6.2.1",
    "papaparse": "^5.1.0",
    "sequelize": "^6.3.5",
    "shacrypt": "^0.1.3",
    "sql-template-strings": "^2.2.2",
    "swagger-ui-express": "^4.1.6",
    "uuid": "^3.3.2",
    "winston": "^3.2.1",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.19.1",
    "mocha": "^6.2.2",
    "nock": "^10.0.6",
    "node-mocks-http": "^1.10.1",
    "nodemon": "^1.18.11",
    "nyc": "^14.0.0",
    "supertest": "^4.0.2"
  },
  "mocha": {
    "use_strict": true,
    "require": [
      "esm"
    ],
    "exit": true
  }
}
