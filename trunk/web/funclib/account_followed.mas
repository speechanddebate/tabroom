<%args>
	$account
</%args>
<%init>


	Tab::Account->set_sql(by_follower => "
		select distinct person.* 
			from person, follow_account
			where follow_account.follower = ?
			and follow_account.account = person.id
			order by person.last
	");

	return Tab::Account->search_by_follower($account->id);

</%init>
