<%args>
	$school
</%args>
<%perl>

	Tab::Account->set_sql(by_school => "
		select distinct person.*
		from person, chapter_admin, school
		where school.id = ? 
		and school.chapter = chapter_admin.chapter
		and chapter_admin.account = person.id
		order by person.last ");


	return Tab::Account->search_by_school($school->id);

</%perl>
