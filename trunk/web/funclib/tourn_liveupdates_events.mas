<%args>
	$tourn
</%args>
<%init>

	Tab::Event->set_sql( liveupdates_by_tourn => "
		select distinct event.*
		from event, setting
		where event.tourn = ? 
		and event.id = setting.event
		and setting.tag = \"live_updates\"
		and setting.type = \"event\"
		order by event.name
	");

	return Tab::Event->search_liveupdates_by_tourn($tourn->id);


</%init>
