<%args>
	$event
</%args>
<%init>
	
	Tab::Judge->set_sql(by_event_strike => "
		select distinct judge.id
		from judge, strike
		where judge.id = strike.judge
		and strike.type = \"event\"
		and strike.event = ? 
		order by judge.id
	");

	return Tab::Judge->search_by_event_strike($event->id);

</%init>
