<%args>
	$group
</%args>
<%init>

	Tab::Setting->set_sql( online_ballot => "
		select count(setting.id) 
		from setting, event
		where event.judge_group = ? 
		and setting.event = event.id
		and setting.tag = \"online_ballots\"
		and setting.value = 1
		and setting.tag = \"event\"
	");

	return Tab::Setting->sql_online_ballot->select_val($group->id);


</%init>
