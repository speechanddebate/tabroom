<%args>
	$panel
</%args>
<%init>

	Tab::Judge->set_sql( judges_by_panel => "
		select distinct judge.id, ballot.chair as chair
		from judge, ballot
		where ballot.judge = judge.id
		and ballot.panel = ?
		group by judge.id
		order by judge.last
	");

	return Tab::Judge->search_judges_by_panel( $panel->id );

</%init>

