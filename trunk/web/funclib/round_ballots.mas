<%args>
	$round
</%args>
<%init>

	Tab::Ballot->set_sql(by_round => "
		select distinct ballot.*
			from panel,ballot,judge
			where panel.round = ? 
			and panel.id = ballot.panel
			and ballot.judge = judge.id
			order by ballot.entry, judge.last
	");

	return Tab::Ballot->search_by_round($round->id);

</%init>
