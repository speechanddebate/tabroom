<%args>
	$round
</%args>
<%init>

	return unless $round;

	Tab::RoomGroup->set_sql( by_round => "
		select distinct room_group.*
		from room_group, room_group_round
		where room_group_round.round = ?
		and room_group_round.room_group = room_group.id
		order by room_group.name");

	return Tab::RoomGroup->search_by_round($round->id);

</%init>
