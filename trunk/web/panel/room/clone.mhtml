<%args>
	$pool_id  => undef
	$clone_id => undef
</%args>
<%init>

	my $counter;
	
	foreach my $rj (Tab::RoomGroupRoom->search( room_group => $clone_id)) { 
		$rj->copy({ room_group => $pool_id });
		$counter++;
	}

	my $msg = $counter." rooms imported into this pool";
	$m->redirect("pool.mhtml?pool_id=$pool_id&msg=$msg");

</%init>
