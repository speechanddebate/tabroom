<%args>
	$jpool_id
	$round_id
	$event_id => undef
	$group_id => undef
</%args>
<%init>
	
	my $jpool_round = Tab::JPoolRound->search( round => $round_id, jpool => $jpool_id)->first;
	$jpool_round->delete;

	$m->redirect("event.mhtml?round_id=$round_id&event_id=".$event_id) if $event_id;;
	$m->redirect("jpool.mhtml?round_id=$round_id&group_id=".$group_id) if $group_id;;

</%init>
