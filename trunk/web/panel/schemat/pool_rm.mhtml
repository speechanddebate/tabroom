<%args>
	$pool_id  => undef
	$round_id => undef
</%args>
<%init>

	my @existings = Tab::RoomGroupRound->search( round => $round_id, room_group => $pool_id );

	foreach my $existing (@existings) { 
		$existing->delete;
	}

	my $pool = Tab::RoomGroup->retrieve($pool_id);
	my $msg = "Round will no longer draw from room pool ".$pool->name;

	$m->redirect("show.mhtml?msg=$msg&round_id=$round_id&settings=1");

</%init>
