<%args>
	$room_id
	$panel_id
	$round_id
	$flight
</%args>
<%init>

	Tab::debuglog("Hey there sailor.  Room $room_id Panel $panel_id Round $round_id");

	$panel_id =~ s/room_//g;

	my $panel = Tab::Panel->retrieve($panel_id);
	my $room = Tab::Room->retrieve($room_id);

	my @already_panels = Tab::Panel->search( room => $room_id, round => $round_id, flight => $flight);

	foreach my $already (@already_panels) { 
		$already->room(0);
		$already->update;
	}

	if ($panel && $room) { 
		$panel->room($room_id);
		$panel->update;
	}

	#return;

</%init>
