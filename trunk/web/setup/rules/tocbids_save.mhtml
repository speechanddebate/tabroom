<%args>
$tourn
@bid_rounds
</%args>

<%init>

  foreach my $bid (@bid_rounds) {
    (my $event_id, my $bid_level) = split(":",$bid);
    my $event = Tab::Event->retrieve($event_id);
    $event->setting("bid_round", $bid_level) if $event;
  }

  $m->redirect("tocbids.mhtml?msg=tournament changes saved");
  </%init>
