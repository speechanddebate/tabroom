<%args>
	$tourn
	$account
	$staff_id
</%args>
<%init>

	my $access = Tab::TournAdmin->retrieve($staff_id);

	my $msg = $access->account->first." ".$access->account->last."'s access has been revoked";

	Tab::log($msg." by ".$account->email);

	$access->delete;

	$m->comp("/funclib/update_calendar.mas", tourn => $tourn);

	$m->redirect("access.mhtml?msg=$msg");

</%init>

