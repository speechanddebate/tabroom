<%args>
	$circuit
	$tourn
</%args>

	<div class="main">

		<h2>Export & save your tournament:</h2>

		<table cellpadding="3" cellspacing="4" width="100%">

			<tr>
				<td>
					<h3>Tabroom.com backup file</h3>
				</td>
			</tr>

			<tr>
				<td>

					<form action="export_tournament.mhtml" method="post">

					Exports all your tournament data file into a .tab file; for
					transfer to another Tabroom.com system or to take a backup

				</td>
			
			
			</tr>
		
			<tr class="liblrow">
				<td class="rightalign">
					<input  type="submit" value="Save .tab File">
					</form>
				</td>
			</tr>

		</table>
		
		<table cellpadding="3" cellspacing="4" width="100%">

			<tr>
				<td>
					<h3>TRPC (Rich Edwards' Debate Program)</h3>
				</td>
			</tr>

			<tr>
				<td>

					<form action="export_trpc_new.mas" method="post">

					Export tournament data for transfer to Rich Edward's Tab
					Room for the PC.  Includes MJP downloads.

				</td>
			
			
			</tr>
		
			<tr class="liblrow">
				<td class="rightalign">
					<input  type="submit" value="Save for TRPC">
					</form>
				</td>
			</tr>

		</table>
		
		<table cellpadding="3" cellspacing="4" width="100%">

			<tr>
				<td>
					<h3>TRIEPC (Rich Edwards' Speech/IE Program)</h3>
				</td>
			</tr>

			<tr>
				<td>

					<form action="export_trie.mas" method="post">
					Export tournament data for transfer to Rich Edward's Tab
					Room/IE for the PC.  

				</td>
			
			
			</tr>
		
			<tr class="liblrow">
				<td class="rightalign">
					<input  type="submit" value="Save for TRIEPC">
					</form>
				</td>
			</tr>

		</table>
		
		
		<table cellpadding="3" cellspacing="4" width="100%">

			<tr>
				<td>
					<h3>Spreadsheets</h3>
				</td>
			</tr>

			<tr>
				<td>

					<form action="export_csv.mhtml" method="post">

					Export a listing of students, events and schools (CSV,
					Excel readable format)

				</td>
			
			
			</tr>
		
			<tr class="liblrow">
				<td class="rightalign">
					<input  type="submit" value="Export Spreadsheet (CSV)">
					</form>
				</td>
			</tr>

		</table>
		
	</div>

	<div class="menu">
		
		<h4>Legacy & Leftovers</h4>

		<h4>Export for CFL Nationals</h4>
			<p>
				Export tournament for NCFL Nationals
				<form action="ncfl_export.mhtml" method="post">
			</p>

			<p style="text-align: right;">
				<input  type="submit" value="Save NCFL Files">
				</form>
			</p>
		
		<h4>Old Style of Debate Tab Room PC</h4>

			<p>
				Export tournament for transfer to Rich Edward's Tab Room for the PC: (Many files, no MJP)
				<form action="export_trpc.mas" method="post">
			</p>

			<p style="text-align: right;">
				<input  type="submit" value="Old School TRPC">
				</form>
			</p>

	</div>

	<div style="clear: both;" />
