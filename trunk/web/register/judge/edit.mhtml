 <%args> 
	$tourn
	$judge_id => undef
	$account
</%args>
<%init>

	my $switch;

	my $judge = Tab::Judge->retrieve($judge_id) if $judge_id;

	$m->abort unless $judge;

	my $school = $judge->school;

	my $diocese = $school->region if $tourn->setting("ncfl");

	my $group = $judge->judge_group;

	my @schools = sort {$a->name cmp $b->name} $tourn->schools;
	my @dioschools;

	if ($diocese) { 
		@dioschools = sort {$a->name cmp $b->name} $diocese->schools(tourn => $tourn->id);
	}

 	my @panels = $m->comp('/funclib/judge_panels.mas', judge => $judge);

	my @online = $m->comp("/funclib/group_online.mas", group => $group);

	my $hired++ unless $judge->school && $judge->school->id;
	
</%init>

	<& /funclib/editor.mas, half => 1 &>

%	if ($hired) { 
		<div class="main">
%	} else {
    	<& "/register/menubar.mas", school => $school, whoami => "judges", tourn => $tourn &>
%	}

	<div class="half">
		<h4>Judge <% $judge ? $judge->first." ".$judge->last : "Add Judge" %></h4>
	</div>

%	if ($judge && $judge->acct_request > 0) { 

%		my $request = $judge->acct_request;

		<h4>An account requests access to this judge</h4>

		<p>This will text or email the user with pairings.  It will also permit
		the user to enter online ballots for this judge.  Be sure that the
		account actually belongs to the judge in question</p>

		<div class="optionblock yellow full">

			<span class="third">
				<% $request->first." ".$request->last %>
			</span>

			<span class="third">
				<a class="white" href="mailto:<% $request->email %>">
				<% $request->email %>
				</a>
			</span>

			<span class="sixth centeralign">
				<a class="dkblue full" href="permit.mhtml?judge_id=<% $judge->id %>&account_id=<% $request->id %>">
					PERMIT
				</a>
			</span>

			<span class="sixth centeralign">
				<a class="dkred full" href="deny.mhtml?judge_id=<% $judge->id %>&account_id=<% $request->id %>">
					DENY
				</a>
			</span>

		</div>

%	} elsif ($judge->account && $judge->account->id) { 

		<div class="half rightalign greentext nowrap">
			<a href="mailto:<% $judge->account->email %>" class="white greentext" title="<% $judge->account->email %>">
				<h5 class="bold">
					Link: <% $judge->account->email %>
				</h5>
			</a>
		</div>

%	}  elsif (@online) { 

		<form action="link.mhtml" method="post">
		<input type="hidden" name="judge_id" value="<% $judge->id %>">

		<div class="half nospace ltyellow">
			<span class="quarter padleft">
				Link To 
			</span>

			<span class="half">
				<input type="email" name="email" size="24" placeholder="Tabroom login email...">
			</span>

			<span class="rightalign quarter">
				<input type="submit" class="thin" value="Link">
			</span>
		</div>

		</form>

%	}  else { 

		<br class="clear">

%	}

		
	<form action="save.mhtml" method="post">
	<input type="hidden" name="judge_id" value="<% ($judge) ? $judge->id : "" %>">

		<div class="half">
			<div class="odd optionblock padtop">
				<span class="third">
					First:
				</span>

				<span class="twothird">
					<input type="text" name="first" size="24" value="<% $judge->first %>" placeholder="First Name">
				</span>
			</div>
		</div>

		<div class="half right">
			<div class="odd optionblock padtop">
				<span class="third">
					Last:
				</span>

				<span class="twothird centeralign">
					<input type="text" name="last" size="24" value="<% $judge->last %>" placeholder="Last Name">
				</span>
			</div>
		</div>


		<div class="half">

			<div class="optionblock <% ($switch++ % 2) ? "odd" : "even" %>">

				<span class="third">
					School
				</span>
				
				<span class="twothird">

					<select name="school" class="fixedmed chosen">

						<option value="">Hired</option> 

%						my %school_already;

% 						if ($diocese) { 
% 							foreach my $school (@dioschools) { 
%								$school_already{$school->id}++;
									<option value="<% $school->id %>" <% ($school->id eq $judge->school->id) ? 'selected' : '' %> > <% $school->short_name %> </option>
%							} 
							<option value="">
								---Other Dioceses:---
							</option>
%						}

% 						foreach my $school (@schools) { 

%							next if $school_already{$school->id}++;

							<option value="<% $school->id %>" <% ($school->id eq $judge->school->id) ? 'selected' : '' %> > <% $school->name%> </option> 

%						} 
	
					</select>
				</span>

			</div>

			<div class="optionblock <% ($switch++ % 2) ? "odd" : "even" %>">

				<span class="third martop">
					Active
				</span> 
				
				<span class="twothird martop">
					
					<label for="Y">
						<span class="padmore third hover">
							<input type="radio" name="active" id="Y" value="1" <% ($judge->active == 1) ? 'checked' : '' %> > 
							Yes
						</span>
					</label>

					<label for="N">
						<span class="padmore third hover">
							<input type="radio" name="active" id="N" value="0" <% ($judge->active != 1) ? 'checked' : '' %> >
							No
						</span>
					</label>

				</span>

			</div> 
			
%			if ($group->setting("tab_ratings")) { 

				<div class="optionblock <% ($switch++ % 2) ? "odd" : "even" %>">

					<span class="third">
						<label for="tab_rating">
							Tab Rating:
						</label>
					</span>

					<span class="twothird">
						<input type="number" name="tab_rating" min="0" max="999" value="<% $judge ? $judge->tab_rating : "" %>">
					</span>
		
				</div>

%			}

			<div class="optionblock <% ($switch++ % 2) ? "odd" : "even" %>">
	
				<span class="third">
					Phone
				</span>	
				
				<span class="twothird">
					<input type="tel" name="phone" size="20" value="<% ($judge->account && $judge->account->id) ? $judge->account->phone : $judge->setting("phone") %>">
				</span>

			</div>

			<div class="optionblock <% ($switch++ % 2) ? "odd" : "even" %>">
	
				<span class="third">
					Email
				</span>	
				
				<span class="twothird">
					<input type="tel" name="email" size="20" value="<% ($judge->account && $judge->account->id) ? $judge->account->email : $judge->setting("email") %>">
				</span>

			</div>

%			if ($tourn->setting("usa_wsdc")) { 

				<div class="optionblock <% ($switch++ % 2) ? "odd" : "even" %>">
		
					<span class="third">
						School:
					</span>	
					
					<span class="twothird">
						<input type="tel" name="real_school" size="20" value="<% $judge->setting("real_school") %>">
					</span>

				</div>

%			}

%			if ($tourn->setting("ncfl")) { 

				<div class="optionblock <% ($switch++ % 2) ? "odd" : "even" %>">
				
					<span class="third">
						Diocese
					</span> 
				
					<span class="twothird">
						<% $judge->school && $judge->school->region ? $judge->school->region->name ."(".$judge->school->region->code.")" : "" %>
					</span> 

				</div>

%			}

			<label for="ada">
			<div class="hover optionblock <% ($switch++ % 2) ? "odd" : "even" %>">

				<span class="third">
					ADA
				</span>

				<span class="twothird">
					<span class="quarter">
						<input type="checkbox" id="ada" class="largecheck" name="ada" value="1" <% $judge && $judge->ada ? 'checked="checked"' : "" %>>
					</span>
					<span class="threequarter explain">
						Requires accessible rooms
					</span>
				</span>
	
			</div>
			</label>

% 			if ($group->setting("neutrals")) { 

				<label for="neutral">
					<div class="hover optionblock <% ($switch++ % 2) ? "odd" : "even" %>">

						<span class="third">
							Neutral
						</span> 

						<span class="twothird">

							<span class="quarter">
								<input type="checkbox" id="neutral" class="largecheck" name="neutral" value="1" <% $judge && $judge->setting("neutral") ? 'checked="checked"' : "" %>>
							</span>

							<span class="threequarter explain">
								May judge own school's entries
							</span>
						</span>

					</div>
				</label>
% 			}

% 			if ($group->setting("track_diversity")) { 

				<label for="diverse">
				<div class="hover optionblock <% ($switch++ % 2) ? "odd" : "even" %>">

					<span class="third">
						Diverse:
					</span> 

					<span class="twothird">

						<span class="quarter">
							<input type="checkbox" id="diverse" class="largecheck" name="diverse" value="1" <% $judge && $judge->diverse ? 'checked="checked"' : "" %>>
						</span>

						<span class="threequarter explain">
							Diversity enhancing judge
						</span>
					</span>

				</div>
				</label>
% 			}

% 			if ($group->setting("first_year_outs")) { 

				<label for="fyo">

					<div class="hover optionblock <% ($switch++ % 2) ? "odd" : "even" %>">

						<span class="third">
							First Year Out
						</span> 

						<span class="twothird">

							<span class="quarter">
								<input type="checkbox" id="fyo" class="largecheck" name="first_year" 
									value="1" <% $judge && $judge->setting("first_year") ? 'checked="checked"' : "" %>>
							</span>

%	 						if ($group->setting("fyo_free_strikes")) { 
								<span class="threequarter explain">
									Automatically a free strike
								</span>
%							}

						</span>

					</div>
				</label>
% 			}

% 			if ($group->setting("prefs")) { 

				<label for="free_strike">
				<div class="hover optionblock <% ($switch++ % 2) ? "odd" : "even" %>">

					<span class="third ">
						Free Strike
					</span> 

					<span class="twothird">

						<span class="quarter">
							<input type="checkbox" id="free_strike" class="largecheck" name="free_strike" 
								value="1" <% $judge && $judge->setting("free_strike") ? 'checked="checked"' : "" %>>
						</span>

% 						if ($group->setting("free_strikes_dont_count")) { 
							<span class="threequarter explain">
								Does not meet obligations
							</span>
% 						}
					</span>

				</div>
				</label>
% 				if ($group->setting("prefs") eq "ndt") { 

					<label for="no_elims">
					<div class="hover optionblock <% ($switch++ % 2) ? "odd" : "even" %>">

						<span class="third ">
							No Elims
						</span> 

						<span class="twothird">

							<span class="quarter">
								<input type="checkbox" id="no_elims" class="largecheck" name="no_elims" 
									value="1" <% $judge && $judge->setting("no_elims") ? 'checked="checked"' : "" %>>
							</span>

							<span class="threequarter explain">
								Removes from NDT Elim Prefs
							</span>
						</span>

					</div>
					</label>

					<label for="list_first_name">
					<div class="hover optionblock <% ($switch++ % 2) ? "odd" : "even" %>">

						<span class="third">
							List First Name:
						</span> 

						<span class="twothird">

							<span class="quarter">
								<input type="checkbox" id="list_first_name" class="largecheck" name="list_first_name" 
									value="1" <% $judge && $judge->setting("list_first_name") ? 'checked="checked"' : "" %>>
							</span>

							<span class="threequarter explain">
								Prints full name on schem
							</span>
						</span>

					</div>
					</label>

%	 			}
% 			}

		</div>

%	undef $switch;

		<div class="half right">

%			unless ($group->setting("no_codes")) { 

					<div class="optionblock <% ($switch++ % 2) ? "odd" : "even" %>">

						<span class="third">
							Code
						</span>

						<span class="twothird">
							<input type="text" name="code" size="10" value="<% $judge->code %>">
						</span>
		
					</div>

%			}

			<div class="optionblock <% ($switch++ % 2) ? "odd" : "even" %>">

				<span class="half">
					Judge group
				</span> 
			
				<span class="half">
					<select name="judge_group" class="fixedsmall chosen">
% 						foreach my $tgroup (sort {$a->name cmp $b->name} $tourn->groups) { 
							<option value="<% $tgroup->id %>" 
								<% ($group && $group->id == $tgroup->id) ? 'selected' : '' %> >
								<% $tgroup->name %> 
							</option>
%						} 

						<option>-----------------------------</option>

					</select> 
				</span> 

			</div> 

			<div class="optionblock <% ($switch++ % 2) ? "odd" : "even" %>">
		
				<span class="half">
					Covers entries in
				</span> 
				
				<span class="half">
					<select name="covers" class="fixedsmall chosen">

						<option value="">
							None Selected
						</option> 

%	 					foreach my $tgroup (sort {$a->name cmp $b->name} $tourn->groups) {
							<option value="<% $tgroup->id %>"
								<% ($judge->covers && $tgroup->id == $judge->covers->id) ? 'selected' : '' %> 
								<% ( ($group && $group->id == $tgroup->id) &! $judge->covers) ? 'selected' : '' %> >
								<% $tgroup->name %> 
							</option>
%						}  

						<option>-----------------------------</option>
					</select>
				</span>

			</div>

			<div class="optionblock <% ($switch++ % 2) ? "odd" : "even" %>">

				<span class="half">
					Special Job
				</span>
				
				<span class="half">
					<input type="text" name="special" size="20" value="<% $judge->special %>">
				</span>

			</div> 
			
			<div class="optionblock <% ($switch++ % 2) ? "odd" : "even" %>">

				<span class="half">
					Coach Notes
				</span>

				<span class="half">
					<input type="text" name="notes" size="20" value="<% $judge->notes %>">
				</span>

			</div>


% 			if ($group->setting("ask_alts")) {

				<div class="optionblock <% ($switch++ % 2) ? "odd" : "even" %>">

					<span class="half">
						Also judges
					</span>
				
					<span class="half">
						<select name="alt_group" class="fixedsmall chosen">

							<option value="">
								None Selected
							</option>

%	 						foreach my $tgroup (sort {$a->name cmp $b->name} $tourn->groups) {

%								next if $tgroup->id == $group->id;
	
								<option value="<% $tgroup->id %>" <% ($judge->alt_group && $tgroup->id == $judge->alt_group->id) ? 'selected' : '' %> >
									<% $tgroup->name %> 
								</option>
%							}  

						</select>

					</span>

				</div>

%			}

%			if ($group->setting("rounds_per")) { 

%			  	my $max_rounds = $group->setting("max_rounds");

					<div class="optionblock <% ($switch++ % 2) ? "odd" : "even" %>">

						<span class="half">
							Round Obligation
						</span>

						<span class="half">
							<input type="number" name="obligation" min="0" max="<% $max_rounds %>" size="10" value="<% $judge->obligation %>">
						</span>
		
					</div>

					<div class="optionblock <% ($switch++ % 2) ? "odd" : "even" %>">

						<span class="half">
							Hired Rounds
						</span>

						<span class="half">
							<input type="number" name="hired" min="0" max="<% $max_rounds - $judge->obligation %>" size="10" value="<% $judge->hired %>">
						</span>
		
					</div>

%				if ($group->setting("exchange")) { 
						<div class="optionblock <% ($switch++ % 2) ? "odd" : "even" %>">

							<span class="half">
								Hired Rds On Offer
							</span>

							<span class="half">
								<input type="number" name="hire_offer" min="0" max="<% $max_rounds - $judge->obligation %>" size="10" value="<% $judge->hire_offer %>">
							</span>
			
						</div>
%				}

%			}


				<div class="optionblock <% ($switch++ % 2) ? "odd" : "even" %>">

					<span class="half smallish">
						Import ID (From JOT/CAT)
					</span>

					<span class="half">
						<input type="number" name="cat_id" size="10" value="<% $judge->cat_id %>">
					</span>

				</div>

% 			if ($group->setting("judge_quals")) {
			
				<div class="full <% ($switch++ % 2) ? "odd" : "even" %> marno">

					<span class="full padless">
						Qualifications:
					</span>

					<span class="full centeralign padless marno">
						<textarea name="qual_history" rows="5" cols="45"><% $judge->setting("qual_history") %></textarea>
					</span>

				</div>
%			}

			</table>

		</div>

		<br class="clear">

		<div class="liblrow rightalign">
			<input  type="submit" value="Save Judge Information">
			</form>
		</div>

%		my @pool_groups;

%		if ($judge->pools) { 
%			foreach my $pool ($judge->pools) { 
%				push @pool_groups, $pool->judge_group;
%			}
%		}

% 		if (($group->pools && $judge) || @pool_groups) { 
		
%			my $switch;
%			my $double;

			<h4>Pools:</h4> 

				<div class="centeralign">
			
				<form action="judge_save_pools.mhtml" method="post">
				<input type="hidden" value="<% $judge->id %>" name="judge_id">

%				push @pool_groups, $judge->judge_group;
%				my %seen = (); 
%				@pool_groups = grep { ! $seen{$_->id} ++ } @pool_groups;

% 				foreach my $group (sort {$a->id <=> $b->id} @pool_groups) { 

%					foreach my $pool ($group->pools) { 

						<label for="pool_<% $pool->id %>">
							<span class="hover <% ($double % 2) ? "odd" : "even" %> third nospace martop">

								<span class="half nowrap">
									<% $pool->name %>
								</span>
								<span class="quarter nowrap">
									<% $pool->judge_group->abbr %>
								</span>

								<span class="sixth">
									<input type="checkbox" class="largecheck" name="<% $pool->id %>" value="1" id="pool_<% $pool->id %>"
										<% Tab::PoolJudge->search( judge => $judge->id, pool => $pool->id) ? 'checked="checked"' : "" %>>
								</span>

							</span>
						</label>

%						$switch++;
%						$double++ unless $switch % 3;

%					} 
%				} 

				<div class="liblrow rightalign">
					<input  class="thin"class="thin"  type="submit" value="Save Pool Changes">
					</form>
				</div> 

			</div> 
				
% 		}

%		if ($group->setting("ask_paradigm") && $judge->account) { 

			<h4>Paradigm</h4>
				
			<div class="even full centeralign">
				<textarea rows="8" name="paradigm" cols="42"><% $judge->account->setting("paradigm") %></textarea>
			</div>
%	 	}

	</div>

	<div class="menu">

		<div class="sidenote">

			<h4>Information</h4>

%			if ($hired) { 

				<a class="blue full" href="/register/judge/roster.mhtml?group_id=<% $group->id %>&hires=1">
					Hired Judges
				</a>

%			} elsif ($judge->school && $group) { 

				<a class="blue full" href="/register/school/judges.mhtml?school_id=<% $judge->school->id %>&judge_group=<% $group->id %>">
					<% $judge->school->short_name %> Judges
				</a>

%			}

%			if ($group) { 
				<a class="blue full" href="/register/judge/roster.mhtml?group_id=<% $group->id %>">
					<% $group->name %> judges roster
				</a>
%			}
			
			<a href="print_sheet.mhtml?judge_id=<% $judge->id %>" class="blue full martop">
				Print <% $judge->first %>'s info sheet
			</a> 
		
%			if ($tourn->setting("ncfl")) { 
				<a href="/funclib/ncfl/print_judge_card.mhtml?judge_id=<% $judge->id %>" class="blue full">
					Print Judge's Info Card 
				</a>
%			}

			<h4>Wreak Havoc</h4>

				<a href="drop.mhtml?judge_id=<% $judge->id %>&school_id=<% ($judge->school) ? $judge->school->id : "" %>" class="yellow full">
					Delete Judge
				</a>

%				if ($judge->account > 0) { 

%					my $warn = "This will render this account unable to post online ballots or get email/text pairings.  You sure?";
				
					<a href="unlink.mhtml?judge_id=<% $judge->id %>" <& "/funclib/confirm.mas", warn => $warn &> class="yellow full">
						Unlink from <% $judge->account->email %>
					</a>

%				} else { 

%				}

		</div>

		<div class="sidenote">

			<h4>Strikes & Prefs</h4>

			<a class="blue full" href="prefs.mhtml?judge_id=<% $judge->id %>">
				Pref Sheets
			</a>

			<a class="blue full" href="conflict_sheet.mhtml?judge_id=<% $judge->id %>">
				Conflict Sheet
			</a>

			<a class="martop yellow full" href="judge_strikes.mhtml?judge_id=<% $judge->id %>">
				Add Strike/Conflict/Time Block
			</a>

%			if ($judge->strikes) { 
				<p class="smaller">Click to delete conflict:</p>
%			}

%  			foreach my $strike (sort {$a->type cmp $b->type} $judge->strikes) {
%				my $warn = "You are about to delete the strike ".$strike->name.".  Are you sure?";
				<a class="yellow full" 	<& "/funclib/confirm.mas", warn => $warn &> href="strike_rm.mhtml?from=edit&strike_id=<% $strike->id %>">
					<% $strike->name ? $strike->name : ""%>
				</a>
%			}

		</div> 

%       if ($judge->judge_group->strike_times) { 

			<div class="sidenote">
    
	            <h4>Times unavailable</h4>
				<p class="nospace explain centeralign">*Will incur fines if below obligation in a block</p>
    
				<form action="strike_save.mhtml" method="post">
				<input type="hidden" value="strike_time" name="type"> 
				<input type="hidden" value="<% $judge->id %>" name="judge_id">
				<input type="hidden" value="1" name="from">

%               foreach my $strike_time ( sort {$a->start->epoch <=> $b->start->epoch} $judge->judge_group->strike_times) {

					<label for="strike_time_<% $strike_time->id %>">
                    <div class="smallish <% ($switch++ % 2) ? "odd" : "even" %> hover" > 

