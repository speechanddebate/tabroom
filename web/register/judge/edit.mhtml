				var judgeID  = $(judgeSpan).attr("judge_id");
				var poolID   = $(judgeSpan).attr("pool_id");

				var postValue, newParent;

				if (parentID === "in") {
					postValue = 0;
					newParent = "out";
				} else {
					postValue = 1;
					newParent = "in";
				}

				$.ajax({
					url: 'jpool_judge_switch.mhtml',
					type: 'POST',
					data: {
						judge_id : judgeID,
						value    : postValue,
						jpool_id : poolID,
