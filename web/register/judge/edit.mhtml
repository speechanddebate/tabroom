%					}
<%perl>
					foreach my $tag (@questionnaires) {

						my $status = "INCOMPLETE";

						my $quiz = Tab::Quiz->retrieve($tag);
						next unless $quiz;

						my $answers = Tab::PersonQuiz->search(
							person => $jperson->id,
							quiz   => $quiz->id
						)->first;

						if (not defined $answers) {
							$status = "NOT STARTED";
						} elsif ($quiz->approval && (not defined $answers->approved_by)) {
							$status = "UNCONFIRMED";
						} elsif ($judge_settings{"form_complete_".$tag} || $answers->completed || $answers->approved_by) {
							$status = "COMPLETE";
						}

						my $tla = ({
							oo   => 1,
							bq   => 1,
							ext  => 1,
							cx   => 1,
							pf   => 1,
							ld   => 1,
							di   => 1,
							hi   => 1,
							poi  => 1,
							ie   => 1,
							nsda => 1,
							toc  => 1,
							ncfl => 1,
							nfhs => 1,
						});

						my $tag = $quiz->tag;
						$tag =~ s/level-two-/L2 /g;
						$tag =~ s/level-one-/L1 /g;
						$tag =~ s/-/ /g;

						my ($start, @words) = split(/\ /, $tag);

						if ($tla->{$start}) {
							$start = uc($start);
						}

						foreach my $word (@words) {
							if ($tla->{$word}) {
								$start .= ' '.uc($word);
							} else {
								$start .= ' '.ucfirst($word);
							}
						}
</%perl>
						<a
							class="full <% $status eq "COMPLETE" ? "blue" : "red" %> flexrow"
							href="questionnaire.mhtml?judge_id=<% $judge->id %>&quiz_id=<% $quiz->id %>"
						>
							<span class='half'>
								<% $start %>
