<%args>
	$tourn
	$tourn_settings
</%args>
<%init>

	unless ($ARGS{'return'}) {
		$m->clear_buffer();
		$r->content_type('application/json');
	}

	$m->comp("/funclib/nsda_entry_status.mas",
		tourn          => $tourn,
		tourn_settings => $tourn_settings
	);

	return "Entry status check complete" if $ARGS{'return'};

	$m->print('{
		"error": false,
		"message": "Entry status check complete"
	}');

	$m->abort();

</%init>
