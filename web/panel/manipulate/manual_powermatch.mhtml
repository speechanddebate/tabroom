#	}

	($entries_hash_ref, $precluded_hash_ref) = $m->comp("/funclib/make_pairing_hash.mas", round_id => $round->id);			

	my %entries = %{$entries_hash_ref};
	my %precluded = %{$precluded_hash_ref};
	
	my $end = time();
