<%args>
	$edit_id => undef
</%args>
<%init>
	
	my $edit = Tab::Person->retrieve($edit_id);

	foreach my $login ($edit->logins) { 

		$m->comp("/funclib/ldap_person.mas", login => $login);

	}

	my $msg = "LDAP Accounts Renewed";

	$m->redirect("person_edit.mhtml?edit_id=".$edit_id."&msg=$msg");

</%init>
