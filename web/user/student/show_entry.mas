						</span>
					</div>
<%perl>
 				}
				undef @panels;
 			}
</%perl>

%			if ($entry->waitlist) {
				<span
					class = "biggish redtext"
				>Waitlisted</span>
%			}

%			if ($event_settings{supp} && $supp_team) {
