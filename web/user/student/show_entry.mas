<%perl>

	my $online_mode = $event_settings{"online_mode"};
	my $now = DateTime->now();

	if (
		$online_mode
		&& $online_mode ne "none"
		&& ($tourn->end > $now)
	) {

		my $prep = $event_settings{"online_prep"};
		my $online_rooms = $tourn_settings->{"online_rooms"};
