		my @rooms;

		foreach my $room (@local_rooms) {

			if (
				$room->{access} eq "entry"
				&& (
					($room->{event_id} < 1)
					|| $room->{event_id} == $event->id
				)
			) {
				push @rooms, $room;
			}
		}

</%perl>
