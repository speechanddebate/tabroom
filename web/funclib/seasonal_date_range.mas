<%args>
	$year       => undef
</%args>

<%init>
	
	unless ($year) { 
		my $this_year = Tab::school_year;
		$year = $this_year->year;
	}
	my $start_string = "07/01/$year";
	my $end_string = "06/30/".($year + 1);
	my $start_dt = Tab::dtme($start_string);	
	my $end_dt = Tab::dtme($end_string);	
	
	return $start_dt, $end_dt;
	
</%init>
