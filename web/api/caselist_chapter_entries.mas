<%args>
	$chapter
</%args>
<%init>

	Tab::Entry->set_sql(by_chapter => 
		"select distinct entry.* 
		from entry, school, event
		where entry.school = school.id
		and entry.dropped = 0
		and entry.waitlist = 0
		and entry.dq = 0
		and entry.unconfirmed = 0
		and school.chapter = ?");

	return Tab::Entry->search_by_chapter($chapter->id);

</%init>
